{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "name": "Boilerplate React Buildless",
  "version": "1.0.0",
  "description": "React buildless boilerplate widget pack",
  "widgets": [
    {
      "name": "example",
      "htmlPath": "./index.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": false,
      "transparent": true,
      "presets": [
        {
          "name": "default",
          "anchor": "top_left",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "45px",
          "monitorSelection": {
            "type": "all"
          }
        }
      ]
    }
  ]
}

