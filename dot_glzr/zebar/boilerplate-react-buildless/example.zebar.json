{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.1/resources/widget-schema.json",
  "htmlPath": "./index.html",
  "zOrder": "normal",
  "shownInTaskbar": false,
  "focused": false,
  "resizable": false,
  "transparent": true,
  "presets": [
    {
      "name": "default",
      "anchor": "top_left",
      "offsetX": "0px",
      "offsetY": "0px",
      "width": "100%",
      "height": "45px",
      "monitorSelection": {
        "type": "all"
      }
    }
  ]
}
